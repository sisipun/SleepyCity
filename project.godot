; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Area2D",
"class": "Cell",
"language": "GDScript",
"path": "res://Game/Level/Cell.gd"
}, {
"base": "Node",
"class": "LevelMap",
"language": "GDScript",
"path": "res://Game/Level/LevelMap.gd"
}, {
"base": "MarginContainer",
"class": "LevelPreview",
"language": "GDScript",
"path": "res://Game/Menu/ChooseLevel/LevelPreview.gd"
}, {
"base": "MarginContainer",
"class": "PackPreview",
"language": "GDScript",
"path": "res://Game/Menu/ChoosePack/PackPreview.gd"
} ]
_global_script_class_icons={
"Cell": "",
"LevelMap": "",
"LevelPreview": "",
"PackPreview": ""
}

[application]

config/name="Cells"
run/main_scene="res://Game/Menu/MainMenu/MainMenu.tscn"

[audio]

default_bus_layout="res://Assets/Common/default_bus_layout.tres"

[autoload]

Storage="*res://Game/Storage.gd"
BackgroundMusic="*res://Game/BackgroundMusic.tscn"

[display]

window/size/width=600
window/size/height=1200
window/size/resizable=false
window/energy_saving/keep_screen_on=false
window/handheld/orientation="sensor_portrait"
window/stretch/mode="2d"
window/stretch/aspect="expand"

[font]

tablet_driver=false

[gui]

theme/custom_font="res://Assets/Common/default.tres"

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
vram_compression/import_pvrtc=true
